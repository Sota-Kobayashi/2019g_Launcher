{

}

void motorControl::setSpeed(const uint16_t& setValue) const
{

}

motorControl::~motorControl()
{

}

constexpr uint16_t motorControl::prescaler
	(const uint16_t resolution = defaultResolution, const uint16_t pwmFreq = defaultFrequency, const uint32_t timerFrequency)
{
	return (timerFrequency / (pwmFreq * resolution));
}
