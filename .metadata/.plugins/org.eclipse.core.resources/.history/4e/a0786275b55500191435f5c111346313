#ifndef MYHEADER_MOTOR_DRIVER_H_
#define MYHEADER_MOTOR_DRIVER_H_

#include "stm32f4xx.h"
#include "stm32f4xx_hal.h"

#include "can.h"

/*モータドライバのコマンドを定義*/
enum drive_command
{
	duty			= 0,
	PID				= 1,
	setParamP		= 2,
	setParamI		= 3,
	setParamD		= 4,
	setParamLIMIT	= 5,
	setParamPPM		= 6
};

void motorDriverSetSpeed(CAN_HandleTypeDef* hcan, uint8_t cmd, int32_t setParam, uint8_t driverAddress);

#endif /* MYHEADER_MOTOR_DRIVER_H_ */
